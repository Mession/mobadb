<h1><%= @list %></h1>
<div class="row">
  <div class="col-md-4">
    <table class="table">
      <thead>
      <tr>
        <th>Name</th>
      </tr>
      </thead>

      <tbody>
      <% @champions.each do |champion| %>
          <tr>
            <td><%= link_to champion.name, champion %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>